<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <StartupObject>TwojeBiuro.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="banner.webp" />
    <None Remove="bannerNative.webp" />
    <None Remove="contact.png" />
    <None Remove="contact_small.png" />
    <None Remove="docs.png" />
    <None Remove="docs_small.png" />
    <None Remove="home.png" />
    <None Remove="home_small.png" />
    <None Remove="icon.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Blazorise.Icons.FontAwesome" Version="1.0.2" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Interactive">
      <HintPath>..\..\..\..\Desktop\Interactive.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="banner.webp" />
    <Resource Include="bannerNative.webp" />
    <Resource Include="contact.png" />
    <Resource Include="contact_small.png" />
    <Resource Include="docs.png" />
    <Resource Include="docs_small.png" />
    <Resource Include="home.png" />
    <Resource Include="home_small.png" />
    <Resource Include="icon.png" />
  </ItemGroup>

</Project>
